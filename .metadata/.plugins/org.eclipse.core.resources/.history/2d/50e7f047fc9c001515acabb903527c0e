public class TimeCounter {

	long startTime;
	long endTime;
	long tDelta;

	public TimeCounter() {
		this.startTime = System.currentTimeMillis();
		System.out.println(startTime);
	}

	

	public long setEndTime(long endTime) {
		this.endTime = endTime;
		tDelta = endTime - startTime;
		return tDelta;
		
	}
	
	
}
