
public class QueryObjectsForCalculationGravPointDistances {
	
	private String uuidFromQuery;
	private int distanceFromGravPoint;

	public QueryObjectsForCalculationGravPointDistances() {
		
		this.uuidFromQuery = uuidFromQuery;
		this.brandName = brandName;
		this.x = x;
		this.y = y;
		
	}

	public String getUuidFromQuery() {
