import java.awt.BorderLayout;

import javax.swing.JFrame;


public class MainFrame extends JFrame {
	
	private JTextArea textArea;
	private JButton btn;
	
	public MainFrame() {
		super ("Hello World");
		
		setLayout(new BorderLayout());
		
		setSize(600, 500);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setVisible(true);
		
	}
}
